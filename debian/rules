#!/usr/bin/make -f

%:
	dh $@

clean:
	make -C client-libraries/erlang clean
	find -name ".svn" -type d -print0 | xargs -0r rm -rf
	dh $@

override_dh_auto_build:
	dh_auto_build
	mkdir -p client-libraries/erlang/ebin
	make -C client-libraries/erlang/src

override_dh_gencontrol:
	erlang-depends
	dh_gencontrol

override_dh_auto_test:
